<div class="day-view">
	<div class="row">
{{!-- 		<div class="col-xs-12">
		    <a href="/month" class="text-left"> &lt; Month </a>
		    <a href="/addEvent" class="text-right"> Add Event </a>
		</div> --}}
	</div>
	<div class="row this-week">
		<div class="cal-row-fluid weekday-name">

			<div class="cal-cell1" id="sun"><small>Sun</small>
			</div>
			<div class="cal-cell1" id="mon"><small>Mon</small>
			</div>
			<div class="cal-cell1" id="tue"><small>Tue</small>
			</div>
			<div class="cal-cell1" id="wed"><small>Wed</small>
			</div>
			<div class="cal-cell1" id="thu"><small>Thu</small>
			</div>
			<div class="cal-cell1" id="fri"><small>Fri</small>
			</div>
			<div class="cal-cell1" id="sat"><small>Sat</small>
			</div>


		</div>
		<div class="cal-row-fluid weekday-num">
			{{!-- {{#if alternate}}
				<a class="pull-left weekdaynav stay" id="lastweek" href="/alt{{lastWeekURL}}">
			{{else}} --}}
				<a class="weekdaynav stay" id="lastweek" href="{{lastWeekURL}}">
			{{!-- {{/if}} --}}
				<span class="glyphicon glyphicon-chevron-left"></span>
			</a>
			{{#each tagDate}}
				{{!-- <div> {{this.0}}</div> --}}
				{{#each this}}
					{{#unless @index}}
						{{!-- <div> {{this}}</div> --}}
					<div class="cal-cell1" id="{{this}}" href="{{this}}">
					{{else}}
						{{!-- <div> {{this}}</div> --}}
						<div class="day-order">{{this}}</div>
					{{/unless}}
				{{/each}}
					</div>
			{{/each}}
		{{!-- 		<div class="cal-cell1" id="sun" href="9">
					<div class="day-order">9</div>
				</div> --}}
			{{!-- {{#if alternate}}
				<a class="pull-right weekdaynav stay" id="nextweek" href="{{nextWeekURL}}">
			{{else}} --}}
				<a class="weekdaynav stay" id="nextweek" href="{{nextWeekURL}}"> hello
			{{!-- {{/if}} --}}
				<span class="glyphicon glyphicon-chevron-right"></span>
			</a>
		</div>

	</div>
	<div class="row">
		<h4 class="text-center", id="fullDate">{{fullDateInString}}</h4>
	</div>

	<div class="row", id="this_day">
		{{!-- Ajax for this day --}}
	</div>

	<div class='col-xs-12 form-group add-event-button'>
		{{!-- Add event button --}}
{{!-- 		<form method='get' action='/addEvent/{{dayId}}' role='form' id='addeventget'>
	        <input type='submit' class='btn btn-primary btn-block' value='Add Event'>
	    </form> --}}
	    <div class='btn btn-primary btn-block btn-check-in' role='form' id='check-in'>
	        Check In
		</div>
	</div>

	<div class='form-group add-event-button'>
		<form method='get' action='#' role='form'>
			<input type='hidden' name='addEventPostSucess' value='{{addEventPostSucess}}' id="addEventPostSucess">
	    </form>
	</div>

	{{!-- Include the cool snazzy check in code --}}
	{{>check-in}}

</div>


